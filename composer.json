{
	"name": "codekandis/authentication",
	"description": "`codekandis/authentication` is a library providing authentication adapters for various authentication methods.",
	"keywords": [
		"php",
		"library",
		"authentication",
		"adapters"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/authentication/issues",
		"docs": "https://github.com/codekandis/authentication/blob/master/README.md",
		"source": "https://github.com/codekandis/authentication/tree/master"
	},
	"require": {
		"php": "^7.4",
		"codekandis/ldap": "^0",
		"codekandis/session": "^0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\Authentication\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\Authentication\\Tests\\": [
				"tests/"
			]
		}
	}
}
