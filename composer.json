{
	"name": "codekandis/authentication",
	"description": "This library provides authentication adapters for various authentication methods.",
	"keywords": [
		"php",
		"library",
		"authentication",
		"adapters"
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/authentication/issues",
		"docs": "https://github.com/codekandis/authentication/blob/master/README.md",
		"source": "https://github.com/codekandis/authentication/tree/master"
	},
	"require": {
		"php": "^7.4"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"sensiolabs/security-checker": "^6",
		"phpunit/phpunit": "^8"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\Authentication\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\Authentication\\Tests\\": [
				"tests/"
			]
		}
	},
	"scripts": {
		"post-update-cmd": [
			"[ ${COMPOSER_DEV_MODE} -eq 0 ] || vendor/bin/security-checker security:check composer.lock"
		]
	}
}
